{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "blog",
    "description": ""
  },
  "options": {
    "draftAndPublish": true,
    "populateCreatorFields": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "minLength": 5
    },
    "slug": {
      "type": "string"
    },
    "url": {
      "type": "string"
    },
    "images": {
      "displayName": "images",
      "type": "component",
      "repeatable": false,
      "component": "shared.images",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true,
      "minLength": 20
    },
    "isFeatured": {
      "type": "boolean",
      "default": false
    },
    "isSuggested": {
      "type": "boolean",
      "default": false
    },
    "isSpecial": {
      "type": "boolean",
      "default": false
    },
    "isPopular": {
      "type": "boolean",
      "default": false
    },
    "readTime": {
      "type": "integer"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "mappedBy": "blogs"
    },
    "continent": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::continent.continent",
      "inversedBy": "blogs"
    },
    "destination": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::destination.destination",
      "inversedBy": "blogs"
    },
    "content": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "required": true,
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "product": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product.product",
      "inversedBy": "blogs"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    },
    "related_blogs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::blog.blog",
      "mappedBy": "blog"
    },
    "blog": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::blog.blog",
      "inversedBy": "related_blogs"
    },
    "profile": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::profile.profile",
      "inversedBy": "blogs"
    }
  }
}

{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "category",
    "pluralName": "categories",
    "displayName": "category",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "minLength": 3,
      "unique": true
    },
    "slug": {
      "type": "string",
      "required": false,
      "unique": true
    },
    "icon": {
      "type": "enumeration",
      "enum": [
        "camp-fire",
        "compass",
        "backpack",
        "bus",
        "beach",
        "location"
      ]
    },
    "blogs": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::blog.blog",
      "inversedBy": "categories"
    }
  }
}
